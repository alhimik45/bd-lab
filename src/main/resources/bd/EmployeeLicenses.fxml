<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab text="Водительские удостоверения">
            <content>
                <BorderPane prefHeight="380.0" prefWidth="519.0">
                    <top>
                        <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                            <children>
                                <Label fx:id="nameInTop" alignment="CENTER" contentDisplay="CENTER" text="Водительские удостоверения">
                                    <font>
                                        <Font size="24.0" />
                                    </font>
                                </Label>
                                <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="34.0" prefWidth="524.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <TextField fx:id="searchLicenses" prefWidth="220.0" promptText="Поиск...">
                                            <HBox.margin>
                                                <Insets left="5.0" right="5.0" />
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </top>
                    <center>
                        <TableView fx:id="licenseTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                            <columns>
                                <TableColumn fx:id="fio" prefWidth="74.0" text="ФИО" />
                                <TableColumn fx:id="cat" prefWidth="134.0" text="Категория" />
                                <TableColumn fx:id="num" minWidth="0.0" prefWidth="53.0" text="Номер удостоверения" />
                                <TableColumn fx:id="date" prefWidth="139.0" text="Дата выдачи" />
                            </columns>
                            <BorderPane.margin>
                                <Insets left="10.0" right="10.0" />
                            </BorderPane.margin>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                        </TableView>
                    </center>
                    <bottom>
                        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="52.0" prefWidth="524.0" BorderPane.alignment="CENTER">
                            <children>
                        <Button fx:id="refr" mnemonicParsing="false" onAction="#updateDrLi" text="Обновить" />
                                <Button fx:id="newLicense" mnemonicParsing="false" text="Новое водительское удостоверение">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0" />
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="editLicense" mnemonicParsing="false" text="Редактировать">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0" />
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="rmLicense" mnemonicParsing="false" text="Удалить">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0" />
                                    </HBox.margin>
                                </Button>
                            </children>
                            <BorderPane.margin>
                                <Insets />
                            </BorderPane.margin>
                        </HBox>
                    </bottom>
                </BorderPane>
            </content>
        </Tab>
        <Tab text="Экзаменационные ведомости">
            <content>
                <BorderPane prefHeight="380.0" prefWidth="519.0">
                    <bottom>
                        <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="52.0" prefWidth="524.0" BorderPane.alignment="CENTER">
                            <children>
                        <Button fx:id="refr1" mnemonicParsing="false" onAction="#updateEx" text="Обновить" />
                                <Button fx:id="newExam" mnemonicParsing="false" text="Новая ведомость">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0" />
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="editExam" mnemonicParsing="false" text="Редактировать">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0" />
                                    </HBox.margin>
                                </Button>
                                <Button fx:id="rmExam" mnemonicParsing="false" text="Удалить">
                                    <HBox.margin>
                                        <Insets left="5.0" right="5.0" />
                                    </HBox.margin>
                                </Button>
                            </children>
                            <BorderPane.margin>
                                <Insets />
                            </BorderPane.margin>
                        </HBox>
                    </bottom>
                    <center>
                        <TableView fx:id="examTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                            <columns>
                                <TableColumn fx:id="fio1" prefWidth="279.0" text="ФИО" />
                                <TableColumn fx:id="result" prefWidth="118.0" text="Оценка" />
                                <TableColumn fx:id="date1" prefWidth="100" text="Дата" />
                            </columns>
                            <BorderPane.margin>
                                <Insets left="10.0" right="10.0" />
                            </BorderPane.margin>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                        </TableView>
                    </center>
                    <top>
                        <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
                            <children>
                                <Label fx:id="nameInTop1" alignment="CENTER" contentDisplay="CENTER" text="Экзаменационные ведомости">
                                    <font>
                                        <Font size="24.0" />
                                    </font>
                                </Label>
                                <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="34.0" prefWidth="524.0" VBox.vgrow="ALWAYS">
                                    <children>
                                        <TextField fx:id="searchExam" prefWidth="220.0" promptText="Поиск...">
                                            <HBox.margin>
                                                <Insets left="5.0" right="5.0" />
                                            </HBox.margin>
                                        </TextField>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </top>
                </BorderPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
